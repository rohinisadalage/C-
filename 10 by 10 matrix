#include <iostream>
using namespace std;
int main() {
    int matrix[10][10];
    for (int i = 0; i < 10; i++) {
        for (int j = 0; j < 10; j++) {
            matrix[i][j] = i * j; // Example: product of row and column indices
        }
    }
    cout << "10x10 Matrix:" << endl;
    for (int i = 0; i < 10; i++) {
        for (int j = 0; j < 10; j++) {
            cout << matrix[i][j] << "\t"; // Use '\t' for better spacing
        }
        cout << endl;
    }
    return 0;
}
